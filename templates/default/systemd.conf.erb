# Dynamically generated by Chef - local modifications will be overwritten

[Unit]
Description=Redis In-Memory Data Store
After=network.target remote-fs.target nss-lookup.target

[Service]
User=<%= node[:redis][:source][:user] ? node[:redis][:source][:user] : "redis" %>
Group=<%= node[:redis][:source][:group] ? node[:redis][:source][:group] : "redis" %>
PIDFile=<%= node[:redis][:pidfile] %>
ExecStart=<%= node[:redis][:source][:prefix] %>/bin/redis-server <%= node[:redis][:config_path] %>
ExecStop=<%= node[:redis][:source][:prefix] %>/bin/redis-cli shutdown
TimeoutStopSec=5
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
